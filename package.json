{
    "name": "svelte-app",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "start": "sirv public --no-clear",
        "jslint": "eslint \"**/*.js\" --ignore-path .gitignore",
        "jslint:fix": "eslint \"**/*.js\" --fix --ignore-path .gitignore",
        "format": "prettier --check . --ignore-path .gitignore",
        "format:fix": "prettier --write . --ignore-path .gitignore",
        "stylelint": "stylelint \"**/*.css\" --ignore-path .gitignore",
        "stylelint:fix": "stylelint \"**/*.css\" --fix --ignore-path .gitignore",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.0.0",
        "@rollup/plugin-replace": "^3.0.0",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-sonarjs": "^0.10.0",
        "husky": "^7.0.2",
        "prettier": "2.4.1",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0"
    },
    "dependencies": {
        "sirv-cli": "^1.0.0",
        "@auth0/auth0-spa-js": "^1.19.3"
    }
}
